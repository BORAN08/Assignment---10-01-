        "def prime_number(n):\n",
        "  if n<2:\n",
        "    print(f\"{n} is not prime number\")\n",
        "  else:\n",
        "    for i in range(2,(n//2)+1):\n",
        "      if n%i==0:\n",
        "        print(f\"{n} is not prime number\")\n",
        "        break\n",
        "    else:\n",
        "      print(f\"{n} is prime number\")\n",
        "prime_number(int(input(\"Please enter a nuber :\")))"
      ],
